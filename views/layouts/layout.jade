//layout.jade
doctype html
html(lang="es")
	head
		title Ubrania
		meta(charset="utf-8")
		link(href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" rel="stylesheet")
		link(href="./css/mycss.css" rel="stylesheet")
		
		script(src="https://code.jquery.com/jquery-3.2.1.min.js")
		script(src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js")
	body
		nav.navbar.navbar-default
			.container-fluid
				.navbar-header
					a.navbar-brand(href="#")
						img(alt="Brand" src="./img/brandimage.png" height="20" width="20")
						
				.navbar-header.navbar-right.navbar-brand
					span.glyphicon.glyphicon-user.dropdown(id="dropdownUser" data-toggle="dropdown" style="font-size:25px")
					ul.dropdown-menu.dropdown-menu-right(aria-labelledby="dropdownUser")
						li
							a(href="#") Profile
						li
							a(href="#") Settings
						li
							a(href="#") Log out
		block pageBody
	footer
		.container.text-center
			hr
			.row
				.col-lg-12
					.col-md-3
						ul.nav.nav-pills.nav-stacked
							li
								a(href="#") About us
							li
								a(href="#") Blog
					.col-md-3
						ul.nav.nav-pills.nav-stacked
							li
								a(href="#") Product for Mac
							li
								a(href="#") Product for Windows
					.col-md-3
						ul.nav.nav-pills.nav-stacked
							li
								a(href="#") Web analytics
							li
								a(href="#") Presentations
					.col-md-3
						ul.nav.nav-pills.nav-stacked
							li
								a(href="#") Product Help
							li
								a(href="#") Developer API
			hr
			.row
				.col-lg-12
					ul.nav.nav-pills.nav-justified
						li
							a(href="#") Â© 2013 Company Name.
						li
							a(href="#") Terms of Service
						li
							a(href="#") Privacy